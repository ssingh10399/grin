#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	int num,m,n;
	cout<<"Enter the number to check whether it is armstrong or not:";
	cin>>num;
	m=num;
	int n1=log10(num)+1;
	while(num!=0)
	{
		n=n+pow((num%10),n1);
		num=num/10;
	}
	if(n==m)
		cout<<"Number is Armstrong.";
	else
		cout<<"Not Armstrong.";
	return 0;
}
